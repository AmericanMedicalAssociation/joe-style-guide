{% extends '@templates/vc-template.twig' %}

{% block pageContent %}

  {% embed "@organisms/vc-layout-paragraphs/vc-layout-paragraphs.twig" with {
    layout_centered: true,
    layout_heading: "Example Heading"
  } %}
    {% block layout_bottom %}
      {% include "@organisms/vc-text-block/vc-text-block.twig" %}
    {% endblock %}
  {% endembed %}

  {% embed "@organisms/vc-layout-paragraphs/vc-layout-paragraphs.twig" with {
    layout_fullwidth: true
  } %}
    {% block layout_bottom %}
      {% include "@organisms/vc-featured-media/vc-featured-media.twig" %}
    {% endblock %}
  {% endembed %}

  {% embed "@organisms/vc-layout-paragraphs/vc-layout-paragraphs.twig" with {
    layout_equal_col: true,
    layout_heading: "Example Heading"
  } %}
    {% block layout_first %}
      {% set text_block = text_block_first %}
      {% include "@organisms/vc-text-block/vc-text-block.twig" %}
    {% endblock %}
    {% block layout_second %}
      {% set text_block = text_block_second %}
      {% include "@organisms/vc-text-block/vc-text-block.twig" %}
    {% endblock %}
  {% endembed %}

{% endblock %}
